{
  "name": "movement-gaming-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "update": "cd ../model && yarn build && cd ../api && yarn add file:../model"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0",
    "@aws-sdk/client-cloudwatch": "^3.616.0",
    "@aws-sdk/client-secrets-manager": "^3.529.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.2",
    "bignumber.js": "^9.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-extended": "^4.0.0",
    "crypto-js": "^4.2.0",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "movement-gaming-model": "file:../model",
    "mysql2": "^3.10.3",
    "nanoid": "3",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bignumber.js": "^5.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/qs": "^6.9.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@typescript-eslint/typescript-estree": "^7.2.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.5",
    "nock": "^13.5.4",
    "prettier": "^3.2.4",
    "qs": "^6.11.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
